        {
            "request": "launch",
            "name": "Deno: invite",
            "type": "node",
            "program": "${workspaceFolder}/supabase/functions/invite/index.ts",
            "cwd": "${workspaceFolder}/supabase/functions/invite",
            "env": {
                "PORT": "8001"
            },
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229,
            "outputCapture": "std",
            "envFile": "${workspaceFolder}/supabase/functions/.env"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch api-test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/api-test/index.js"
        },
        {
            "request": "attach",
            "name": "Attach to Deno",
            "type": "node",
            "outputCapture": "std",
            "address": "supabase.squadquest.app",
            "port": 9229,
            "localRoot": "${workspaceFolder}/supabase/functions",
            "remoteRoot": "/home/deno/functions",
            "restart": true,
            "timeout": 1000000,
            "sourceMapPathOverrides": {
                "file:///home/deno/functions/*": "${workspaceFolder}/supabase/functions/*",
            }
        },
    ]
}